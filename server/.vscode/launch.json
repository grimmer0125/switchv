{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "start:debug"
            ],
            "console": "integratedTerminal"
        }
    ]
}